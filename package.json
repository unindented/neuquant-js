{
  "name": "neuquant-js",
  "description": "NeuQuant neural-network image quantization algorithm.",
  "version": "0.2.0",
  "license": "MIT",
  "main": "helpers.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/neuquant-js.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist",
    "lint": "eslint src test",
    "test:node": "mocha --compilers js:babel-register",
    "test:browser": "karma start --single-run",
    "test": "npm run test:node && npm run test:browser",
    "build:lib": "babel src --out-dir .",
    "build:dev": "webpack --config webpack.config.development.js",
    "build:prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dev && npm run build:prod",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.0",
    "node-libs-browser": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "pixel-stream": "^1.0.3"
  }
}
